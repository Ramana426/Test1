    angular.module("myapp", [])
      .controller("MyController", function($scope) {
        $scope.register = {};
        $scope.register.country = "";
       
        $scope.register.countries = [{
          id: "1",
          name: "Service Address (Unparsed)"
        }, {
          id: "2",
          name: "Service Address (parsed)"
        }, {
          id: "3",
