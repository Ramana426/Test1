<script>


$(document).ready(function(){
 $('#purpose').click(function() {
 if ( this.value == '1'){
 $("#unparsed").show();
		$("#validation").show();}
		
 });
   $("#unparsed").show();
		$("#validation").show();
    $('#purpose').on('change', function() {
      
       if ( this.value == '2')
      {
        $("#parsed").show();
		$("#addressid").hide();
		$("#officeid").hide();
		$("#domestic").hide();
		$("#validation2").hide();
		$("#latitude").hide();
		$("#singlelineentry").hide();
		$("#batchfile").hide();
        $("#unparsed").hide();
		$("#validation1").hide();
		$("#validation").show();
      }
	  else  if ( this.value == '3')
      {
        $("#parsed").hide();
		$("#addressid").show();
		$("#officeid").hide();
		$("#domestic").hide();
		$("#validation2").hide();
		$("#latitude").hide();
		$("#singlelineentry").hide();
		$("#batchfile").hide();
        $("#unparsed").hide();
		$("#validation1").show();
		$("#validation").hide();
      }
	  else  if ( this.value == '4')
      {
        $("#parsed").hide();
		$("#addressid").hide();
		$("#officeid").show();
		$("#domestic").hide();
		$("#latitude").hide();
		$("#validation2").hide();
		$("#singlelineentry").hide();
		$("#batchfile").hide();
        $("#unparsed").hide();
		$("#validation1").hide();
		$("#validation").show();
      }
	  else  if ( this.value == '5')
      {
        $("#parsed").hide();
		$("#addressid").hide();
		$("#officeid").hide();
		$("#domestic").show();
		$("#latitude").hide();
		$("#singlelineentry").hide();
		$("#batchfile").hide();
        $("#unparsed").hide();
		$("#validation2").hide();
		$("#validation1").hide();
		$("#validation").show();
      }
	  else  if ( this.value == '6')
      {
        $("#parsed").hide();
		$("#addressid").hide();
		$("#officeid").hide();
		$("#domestic").hide();
		$("#latitude").show();
		$("#singlelineentry").hide();
		$("#batchfile").hide();
        $("#unparsed").hide();
		$("#validation1").show();
		$("#validation").hide();
		$("#validation2").hide();
      }
	  else  if ( this.value == '7')
      {
        $("#parsed").hide();
		$("#addressid").hide();
		$("#officeid").hide();
		$("#domestic").hide();
		$("#latitude").hide();
		$("#singlelineentry").show();
		$("#batchfile").hide();
        $("#unparsed").hide();
		$("#validation1").show();
		$("#validation").hide();
		$("#validation2").hide();
      }
	  else  if ( this.value == '8')
      {
        $("#parsed").hide();
		$("#addressid").hide();
		$("#officeid").hide();
		$("#domestic").hide();
		$("#latitude").hide();
		$("#singlelineentry").hide();
		$("#batchfile").show();
        $("#unparsed").hide();
		$("#validation1").show();
		$("#validation").hide();
		$("#validation2").show();
      }
	  else  if ( this.value == '9')
      {
        $("#parsed").hide();
		$("#addressid").hide();
		$("#officeid").hide();
		$("#domestic").hide();
		$("#latitude").hide();
		$("#singlelineentry").hide();
		$("#batchfile").hide();
        $("#unparsed").hide();
		$("#insertcustomersintolrds").show();
		$("#validation1").show();
		$("#validation").hide();
		$("#validation2").hide();
      }
       else  
      {
        $("#parsed").hide();
		$("#addressid").hide();
		$("#officeid").hide();
		$("#domestic").hide();
		$("#latitude").hide();
		$("#singlelineentry").hide();
		$("#batchfile").hide();
        $("#unparsed").hide();
		$("#insertcustomersintolrds").hide();
		$("#validation1").hide();
		$("#validation").hide();
		$("#validation2").hide();
      }
    });
});

	  </script>
